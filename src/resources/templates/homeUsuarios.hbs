{{#block "links-header"}}
    <link rel="stylesheet" href="/css/homePage.css">
{{/block}}

{{> templates/partials/headerUsuario}}


{{#partial "header-izquierdo"}}
    <li class="nav-item">
        <a class="nav-link active py-0" href="/home">Home</a>
    </li>
    <li class="nav-item">
        <a class="nav-link  py-0" href="/preguntasFrecuentes" target="_self">Preguntas Frecuentes</a>
    </li>
{{/partial}}

{{#partial "contenido"}}
    {{#ifEquals tipoUsuario "COLABORADOR_HUMANO"}}
        {{> templates/colaboradorHumano/homeColabHumano}}
    {{else}}
        {{#ifEquals tipoUsuario "COLABORADOR_JURIDICO"}}
            {{> templates/colaboradorJuridico/homeColabJuridico}}
        {{else}}
            {{#ifEquals tipoUsuario "TECNICO"}}
                {{> templates/tecnico/homeTecnico}}
            {{else}}
                {{> templates/admin/homeAdmin}}
            {{/ifEquals}}
        {{/ifEquals}}
    {{/ifEquals}}
{{/partial}}
{{> templates/base}}